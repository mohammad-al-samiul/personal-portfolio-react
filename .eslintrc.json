{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": "off",
    "no-console": 0,
    "react/state-in-constructor": 0,
    "indent": 0,
    "linebreak-style": 0,
    "react/prop-types": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "global-require": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "quotes": "off",
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    // "import/no-self-import": 0,
    "arrow-body-style": 0,
    "no-var": "error",
    "camelcase": "error",
    "no-empty": "error",
    "default-case": "error",
    "eqeqeq": "error",

    "max-depth": ["error", 3]
  }
}
